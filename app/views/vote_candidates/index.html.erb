<p id="notice"><%= notice %></p>

<h1>Listing Vote Candidates</h1>

<table>
  <thead>
    <tr>
      <th>Candidate</th>
      <th>Vote</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_candidates.each do |vote_candidate| %>
      <tr>
        <td><%= vote_candidate.candidate %></td>
        <td><%= vote_candidate.vote %></td>
        <td><%= link_to 'Show', vote_candidate %></td>
        <td><%= link_to 'Edit', edit_vote_candidate_path(vote_candidate) %></td>
        <td><%= link_to 'Destroy', vote_candidate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote candidate', new_vote_candidate_path %>
