<p id="notice"><%= notice %></p>

<h1>Listing Activist Fronts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activist_fronts.each do |activist_front| %>
      <tr>
        <td><%= link_to 'Show', activist_front %></td>
        <td><%= link_to 'Edit', edit_activist_front_path(activist_front) %></td>
        <td><%= link_to 'Destroy', activist_front, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activist front', new_activist_front_path %>
